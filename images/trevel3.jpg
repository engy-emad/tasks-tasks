/*Header*/
#plasma-wide-wrapper{color:#fff;padding:30px 0;margin:0 auto}
#header-inner{background-position:left;background-repeat:no}
.plasma-wide img{height:auto;max-height:100%;margin:0}
.plasma-wide h1,.plasma-wide h1 a,.plasma-wide h1 a:hover,.plasma-wide h1 a:visited{font-family:'Comfortaa', sans-serif;color:#EE5B32;font-size:52px;font-weight:400;line-height:1.2em;margin:0;padding:0 0 5px;text-decoration:none;text-transform:uppercase;letter-spacing:1px}
